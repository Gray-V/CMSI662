#include <iostream>
#include <vector>

void printVector(const std::vector<int>& vec) {
  for (size_t i = 0; i < vec.size(); ++i) {
    std::cout << vec[i] << " ";
  }
  std::cout << std::endl;
}

int main() {
  std::vector<int> numbers = {1, 2, 3, 4, 5};

  for (size_t i = 0; i <= numbers.size(); ++i) {
    numbers[i] *= 2;
  }

  printVector(numbers);
  return 0;
}
